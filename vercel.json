{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "crons": [
        {
            "path": "/api/cron/check-laporan-cocok",
            "schedule": "0 0 * * *"
        }
    ]
}